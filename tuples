tuple1 = ("apple", "banana", "cherry")
tuple2 = (1,2,3)

tuple3 = (tuple1+tuple2)
print(tuple3)

tuple1 = ("apple", "banana", "cherry")
myfruits = (tuple1*2)
print(myfruits)

thistuple = (1,2,3,4,4,4,5,6,7,8)
x = thistuple.count(4)
print(x)

thistuple = (1,2,3,3,3,4,5,6,7)
x = thistuple.index(5)
print(x)
